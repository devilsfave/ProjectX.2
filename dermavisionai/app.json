{
  "expo": {
    "name": "dermavisionai",
    "slug": "dermavisionai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Assets.xcassets/AppIcon.appiconset/1024.png",
    "scheme": "dermavisionai",  
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash screen/splash_screen.gif",
      "resizeMode": "contain",
      "backgroundColor": "#1A0C27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devilsfave.dermavisionai",
      "icon": "./assets/Assets.xcassets/AppIcon.appiconset/1024.png",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to analyze skin conditions." 
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appstore.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./assets/appstore.png",
      "package": "com.devlin_39.dermavisionai",
      "permissions": ["CAMERA"] 
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/Assets.xcassets/AppIcon.appiconset/196.png"
    },
    "description": "DermaVisionAI helps you analyze skin conditions using AI.", 
    "primaryColor": "#1A0C27", 
    "updates": {
      "fallbackToCacheTimeout": 0 
    },
    "assetBundlePatterns": [
      "**/*" 
    ],
    "plugins": [
      "expo-router",
      "expo-camera",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
        "recordAudioAndroid": true
      },
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/MaterialIcons.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "238e5f85-22af-486d-8c6a-7885ef3ddde1"
      }
    },
    "owner": "devlin_39"
  }
}
